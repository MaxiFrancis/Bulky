@model List<Category>

<h1>Category list</h1>
<table class="table table-bordered table-striped">
    <thead></thead>
    <tr>
        <th>
            Category Name
        </th>
        <th>
            Desplay Order
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model.OrderBy(u => u.DisplayOrder)){
            <tr>
            <th>
                @obj.Name
            </th>
            <th>
                @obj.DisplayOrder
            </th>
        </tr>
        }
    </tbody>
</table>